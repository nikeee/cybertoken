{
  "name": "cybertoken",
  "version": "5.1.0",
  "description": "A token format for APIs inspired by the GitHub's API token format.",
  "keywords": [
    "api",
    "format",
    "generator",
    "secret",
    "token"
  ],
  "license": "ISC",
  "author": "Niklas Mollenhauer",
  "repository": {
    "type": "git",
    "url": "https://github.com/nikeee/cybertoken.git"
  },
  "bin": "dist/cli.mjs",
  "type": "module",
  "main": "dist/index.mjs",
  "types": "dist/index.d.mts",
  "scripts": {
    "build": "tsdown",
    "ci": "tsc --noEmit && oxlint --deny-warnings -f github",
    "docs": "typedoc",
    "format": "oxfmt",
    "lint": "oxlint",
    "lint:fix": "oxlint --fix",
    "test": "tsc --noEmit && node --test",
    "test:coverage": "node --test --experimental-test-coverage",
    "prepublishOnly": "npm run build"
  },
  "devDependencies": {
    "@types/node": "^25.2.2",
    "expect": "^30.2.0",
    "oxfmt": "^0.34.0",
    "oxlint": "^1.49.0",
    "tsdown": "^0.20.3",
    "typedoc": "^0.28.16"
  },
  "engines": {
    "node": ">=20"
  }
}
